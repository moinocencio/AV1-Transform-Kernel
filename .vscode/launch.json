    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "aomenc_test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Developed_Work/Libaom/Builds/Debug_Build/aomenc",
            "args": [
                "--limit=20",
                "-h", "288",
                "-w", "352",
                "-p", "1",
                "-o", "${workspaceFolder}/Developed_Work/Libaom/Test_Videos/waterfall_test.webm",
                "${workspaceFolder}/Developed_Work/Libaom/Test_Videos/waterfall_cif.y4m"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}